{
  "version": 2,
  "buildCommand": "cd ../.. && npm install && npm run build --workspace=apps/api",
  "installCommand": "cd ../.. && npm install",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}